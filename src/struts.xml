<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<include file="struts-default.xml"></include>
	<package name="" namespace="/" extends="struts-default">
		<action name="">
			<result>/WEB-INF/index.jsp</result>
		</action>
		<action name="home">
			<result>/WEB-INF/index.jsp</result>
		</action>
		<action name="family" class="it.mondovich.actions.FamilyAction">
			<result>/WEB-INF/family.jsp</result>
		</action>
		<action name="new" method="newPerson" class="it.mondovich.actions.FamilyAction">
			<result>/WEB-INF/family.jsp</result>
		</action>
		<action name="delete" method="deletePerson" class="it.mondovich.actions.FamilyAction">
			<result>/WEB-INF/family.jsp</result>
			<result name="ERROR">/WEB-INF/family.jsp</result>
		</action>
		<action name="bankaccount" class="it.mondovich.actions.BankAccountAction">
			<result>/WEB-INF/bankaccount.jsp</result>
		</action>
		<action name="reports" class="it.mondovich.actions.ReportAction">
			<result>/WEB-INF/reports.jsp</result>
		</action>
	</package>
</struts>
